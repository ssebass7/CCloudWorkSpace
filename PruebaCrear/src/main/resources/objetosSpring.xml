<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="email1" class="pruebaCrear.entidad.Email">
<property name="id" value="1"></property>
<property name="destinatario" value="son goku"></property>
<property name="remitente" value="freezer"></property>
</bean>

<bean id="email2" class="pruebaCrear.entidad.Email">
<property name="id" value="2"></property>
<property name="destinatario" value="vegeta"></property>
<property name="remitente" value="radix"></property>
</bean>

<bean id="autor1" class="pruebaCrear.entidad.Autor">
<property name="id" value="1"></property>
<property name="nombre" value="cervantes"></property>
<property name="edad" value="573"></property>
</bean>

<bean id="libro1" class="pruebaCrear.entidad.Libro">
<property name="isbn" value="123"></property>
<property name="editorial" value="santillana"></property>
<property name="autor" ref="autor1"></property>
</bean>
</beans>
